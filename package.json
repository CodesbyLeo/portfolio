{
  "name": "portfolio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "sass": "sass --watch sass/main.scss:css/main.css",
    "dev:all": "npm-run-all --parallel dev sass",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "nightwatch tests/e2e",
    "test:unit": "nightwatch src/components/**/__tests__/*",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/head": "^2.0.0",
    "axios": "^1.7.3",
    "date-fns": "^3.6.0",
    "emailjs-com": "^3.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-matomo": "^4.2.0",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@nightwatch/vue": "0.4.5",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/nightwatch": "^2.3.25",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.19",
    "chromedriver": "^123.0.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "geckodriver": "^4.2.1",
    "nightwatch": "^3.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.1",
    "vite": "^4.5.5",
    "vite-plugin-vue-devtools": "^7.0.25",
    "wait-on": "^7.0.1"
  }
}
